\n  }\n  img {\n    width: 100%;\n    height: 90vh;\n    object-fit: cover;\n  }\n`;\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem;\n  align-items: center;\n  justify-content: space-around;\n  @media (max-width: 1300px) {\n    display: block;\n    margin: 2rem 2rem;\n  }\n`;\n\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    margin: 1rem 0rem;\n    height: 0.5rem;\n  }\n`;\n\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\n\n//Award Component\n\nconst Award = ({ title, description, award }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\nexport default MovieDetail;\n","/Users/oluwadahunsialawode/Desktop/Course/tutorials/React tutorial/Capture Portfolio /capture/src/components/movieState.js",[],"/Users/oluwadahunsialawode/Desktop/Course/tutorials/React tutorial/Capture Portfolio /capture/src/animation.js",[],"/Users/oluwadahunsialawode/Desktop/Course/tutorials/React tutorial/Capture Portfolio /capture/src/components/Wave.js",[],"/Users/oluwadahunsialawode/Desktop/Course/tutorials/React tutorial/Capture Portfolio /capture/src/components/Toggle.js",[],["86","87"],"/Users/oluwadahunsialawode/Desktop/Course/tutorials/React tutorial/Capture Portfolio /capture/src/components/useScroll.js",[],"/Users/oluwadahunsialawode/Desktop/Course/tutorials/React tutorial/Capture Portfolio /capture/src/components/ScrollTop.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":1,"column":17,"nodeType":"94","messageId":"95","endLine":1,"endColumn":25},{"ruleId":"92","severity":1,"message":"96","line":12,"column":18,"nodeType":"94","messageId":"95","endLine":12,"endColumn":27},{"ruleId":"88","replac